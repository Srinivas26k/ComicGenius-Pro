{"name": "comicgenius-pro", "version": "1.2.0", "description": "AI-Powered Comic Creator with Character Consistency using Gemini 2.5 Flash Image Preview - Electron + Tailwind UI", "main": "app.js", "scripts": {"dev:web": "concurrently \"npm:tailwind:watch\" \"serve .\"", "tailwind:build": "tailwindcss -i ./styles.css -o ./dist/app.css --minify", "tailwind:watch": "tailwindcss -i ./styles.css -o ./dist/app.css --watch", "dev:desktop": "concurrently \"npm:tailwind:watch\" \"cross-env NODE_ENV=development electron ./desktop/main.js\"", "build:desktop": "npm run tailwind:build && electron-builder", "postinstall": "electron-builder install-app-deps || true"}, "keywords": ["gemini", "ai", "comic", "storytelling", "image-generation", "electron", "tailwind", "neo-brutalism"], "author": "Hackathon Team", "license": "MIT", "dependencies": {"@google/genai": "^0.7.0", "jspdf": "^2.5.1"}, "devDependencies": {"serve": "^14.2.0", "autoprefixer": "^10.4.19", "concurrently": "^8.2.2", "cross-env": "^7.0.3", "electron": "^32.1.2", "electron-builder": "^25.1.8", "keytar": "^7.9.0", "postcss": "^8.4.40", "tailwindcss": "^3.4.10", "tw-animate-css": "^0.4.0"}, "build": {"appId": "com.comicgenius.pro", "productName": "ComicGenius Pro", "directories": {"buildResources": "assets"}, "files": ["desktop/**/*", "index.html", "dist/app.css", "app.js", "src/**/*", "assets/**/*", "styles.css", "tailwind.config.cjs", "postcss.config.cjs"], "mac": {"category": "public.app-category.graphics-design"}, "win": {"target": "nsis"}, "linux": {"category": "Graphics"}}}